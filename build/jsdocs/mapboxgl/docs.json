{
    "opts": {
        "recurse": true,
        "template": "../template",
        "configPath": "../mapboxgl",
        "destination": "./docs/mapboxgl"
    },
    "disqus":"",
    "tags": {
        "allowUnknownTags": true,
        "linkdoc-mapboxgl":"https://www.mapbox.com/mapbox-gl-js/api/"
    },
    "i18n": {
        "defaultLang": "en",
        "possibleLang": ["en"],
        "dbFilePath":"./build/jsdocs/mapboxgl"
    },
    "source": {
        "includePattern": ".+\\.js?$",
        "excludePattern": ".+[\\\\/]src[\\\\/]common[\\\\/]iServer[\\\\/].*Service.js?$",
        "include": [
            "src/common","src/mapboxgl"
        ],
        "exclude": [
            "src/common/overlay/levelRenderer",
            "src/common/lang/locales"
        ]
    },
    "plugins": ["plugins/markdown","../plugins/usage"],
    "markdown": {
        "tags": ["usage"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        },
        "namespace": "mapboxgl.supermap",
        "npm":"@supermap/iclient-mapboxgl",
        "cdn":"https://iclient.supermap.io/dist/mapboxgl/iclient-mapboxgl.js"
    }
}
